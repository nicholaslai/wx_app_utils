.calendar-container .table {
	width: 100%;
	border-top: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	font-family: arial;
}
.calendar-container .tr {
	width: 100%;
	display: flex;
	justify-content: space-around;
}
.calendar-container .td {
	flex: 0 0 1;
	text-align: center;
	width: 80rpx;
	height: 80rpx;
	line-height: 80rpx;
	vertical-align: center;

}
.calendar-container .disabled {
	color: #ccc;
}

.calendar-container .selectedDate {
	background-color: green;
	border-radius: 3px;
	color: #fff;
}


.calendar-container .nav-btn-group {
	justify-content: space-around;
	display: flex;
	text-align: center;
	padding: 15rpx 5rpx;
	border-top: 1px solid #ccc;
}
.calendar-container .nav-btn-group-item {
	flex: 1;
	font-family: arial;
}
.calendar-container .close-btn-group {
	text-align: right;
	padding: 15rpx 5rpx;
}

.calendar-container .close-btn-group .icon {
	font-size: 50rpx;
	color: #666;
}
.calendar-container .nav-btn-group .icon {
	font-size: 50rpx;
	color: #666;
}
